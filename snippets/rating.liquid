{% assign rating = product.metafields.custom.rating | default: 0 %}
<div class="product-rating" aria-label="Product rating: {{ rating }} out of 5">
  <div class="star-container">
  {% for i in (1..5) %}
    {% if i <= rating %}
      <span class="star filled" aria-hidden="true">&#9733;</span>
    {% else %}
      <span class="star" aria-hidden="true">&#9734;</span>
    {% endif %}
  {% endfor %}
</div>
  <p class="rating-count">4.7/223</p>
</div>